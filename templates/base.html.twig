<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
    and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
    {#{{ encore_entry_link_tags('app') }}#}
    {% endblock %}
    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" rel="stylesheet" />
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <!-- MDB -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.3.0/mdb.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="/css/main.css">
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    {% block javascripts %}
    {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}
</head>

<body>
    {% include "includes/navbar.html.twig" %}

    {% block body %}{% endblock %}

    
                    {# fixed el#}
                    <div class="fixed-action-btn" id="fixed1" style="height: 80px;">
                        <a class="btn btn-floating btn-primary btn-lg ripple-surface" style="background-color: #f44336" data-mdb-toggle="modal" data-mdb-target="#exampleModal">
                          <i class="fas fa-pencil-alt"></i>
                        </a>
                  
                        <ul class="list-unstyled" style="margin-bottom: 80px; transform: translateY(368px);">
                          <li>
                            <a class="btn btn-primary btn-floating btn-lg ripple-surface" style="background-color: #f44336"><i class="fas fa-star"></i></a>
                          </li>
                          <li>
                            <a class="btn btn-primary btn-floating btn-lg" style="background-color: #fdd835"><i class="fas fa-user"></i></a>
                          </li>
                          <li>
                            <a class="btn btn-primary btn-floating btn-lg ripple-surface" style="background-color: #4caf50"><i class="fas fa-envelope"></i></a>
                          </li>
                          <li>
                            <a class="btn btn-primary btn-floating btn-lg ripple-surface" style="background-color: #2196f3"><i class="fas fa-shopping-cart"></i></a>
                          </li>
                        </ul>
                      </div>
                      {# fixed #}

    <!-- MDB -->
<script
type="text/javascript"
src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.3.0/mdb.min.js"
></script>
<script>
  AOS.init();
</script>
</body>

</html>